# macOS keybindings:
'.platform-darwin atom-workspace atom-text-editor:not([mini])':
  'ctrl-alt-o': 'clojure-repl:startRepl'
  'cmd-enter': 'clojure-repl:sendToRepl'

'.platform-darwin atom-workspace atom-text-editor:not([mini]).repl-entry':
  'cmd-up': 'clojure-repl:showOlderHistory'
  'cmd-down': 'clojure-repl:showNewerHistory'


# Linux keybindings:
'.platform-linux atom-workspace atom-text-editor:not([mini])':
  'ctrl-alt-o': 'clojure-repl:startRepl'
  'ctrl-enter': 'clojure-repl:sendToRepl'

'.platform-linux atom-workspace atom-text-editor:not([mini]).repl-entry':
  'ctrl-up': 'clojure-repl:showOlderHistory'
  'ctrl-down': 'clojure-repl:showNewerHistory'
